{
  "name": "npm-package-starter",
  "version": "1.0.0",
  "description": "A starter code for npm package with typescript",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "publish": "npm run test && npm login && npm publish --access public",
