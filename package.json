  "engines": {
    "node": ">=16.x"
  },
  "bugs": {
    "url": "https://github.com/smflow/npm-package-starter/issues"
  },
  "homepage": "https://github.com/smflow/npm-package-starter#readme",
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-typescript": "^7.23.3",
